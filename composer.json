{
    "name": "hatchetaustralia/fakerphp-loremflickr-kittens",
    "description": "Alternative image provider for fakerphp using loremflickr.com ",
    "keywords": [
        "faker",
        "php",
        "images",
        "loremflickr"
    ],
    "homepage": "https://github.com/hatchetaustralia/fakerphp-loremflickr-kittens",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Hare",
            "email": "matt@hatchet.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Hatchet\\FakerLoremFlickrKittens\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hatchet\\FakerLoremFlickrKittens\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
